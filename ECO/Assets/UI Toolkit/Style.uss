.container {
    width: 100%;
    height: 100%;
    min-width: 100%;
    min-height: 100%;
    align-items: stretch;
    flex-direction: column;
}

.down {
    align-items: flex-end;
    width: 100%;
    height: 10%;
    max-height: none;
    background-color: rgb(132, 132, 132);
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    align-self: center;
    flex-wrap: nowrap;
}

.note {
    min-width: 8%;
    min-height: 100%;
    max-width: none;
    justify-content: flex-start;
    max-height: 100%;
    align-items: flex-start;
    margin-left: 10px;
}

.top {
    width: 100%;
    height: 15%;
    align-items: flex-start;
    justify-content: flex-end;
    align-self: auto;
    margin-left: 20px;
}

.note-active {
    min-width: 10%;
    min-height: 120%;
}

.middle {
    height: 100%;
    width: 100%;
    align-items: center;
}

.unity-progress-bar {
    min-width: 150px;
    background-color: rgb(236, 14, 14);
    border-left-color: rgb(106, 15, 15);
    border-right-color: rgb(106, 15, 15);
    border-top-color: rgb(106, 15, 15);
    border-bottom-color: rgb(106, 15, 15);
    border-left-width: 2px;
    border-right-width: 2px;
    border-top-width: 2px;
    border-bottom-width: 2px;
    border-top-left-radius: 2px;
    border-bottom-left-radius: 2px;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
}

.center {
    width: 50%;
    height: 100%;
    justify-content: center;
    align-items: center;
    align-self: auto;
    flex-direction: column;
}

.container-center {
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
}

.buttons-center {
    width: 50%;
    align-items: center;
    max-width: 10px;
    justify-content: flex-start;
}

.unity-text-element {
}

.unity-button {
    background-color: rgba(0, 0, 0, 0);
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    transition-duration: 0.1s;
    transition-timing-function: ease-in;
    min-width: 100px;
}

.title {
    width: 100%;
    height: 30%;
    margin-top: 100px;
    -unity-font: url('project://database/Assets/UI%20Toolkit/UnityThemes/UnityDefaultRuntimeTheme.tss?fileID=2230732570650464555&guid=e7081d85b89a1ee49b50863222b1d6e4&type=3#NotInter-Regular');
    color: rgb(255, 255, 255);
    font-size: 70px;
    -unity-text-align: upper-center;
    text-overflow: clip;
    -unity-font-style: bold;
}

.unity-button:hover {
    color: rgb(255, 255, 255);
    translate: 10px 0;
    scale: 1.2 1.2;
    rotate: 10deg;
    transition-duration: 0.3s;
    transition-timing-function: ease-out;
}

.unity-button:focus {
    -unity-background-image-tint-color: rgb(0, 0, 0);
    background-color: rgb(255, 255, 255);
    translate: 0 0;
    scale: 1 1;
    rotate: 0;
}

.esc-active {
    background-color: rgb(0, 0, 0);
    opacity: 0.46;
    position: absolute;
    width: 100%;
    height: 100%;
    min-width: 100%;
    min-height: 100%;
    right: auto;
    bottom: auto;
    visibility: visible;
    overflow: visible;
    display: flex;
}
